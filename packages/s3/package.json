{
  "name": "@fluxmedia/s3",
  "version": "1.0.1",
  "description": "AWS S3 provider for FluxMedia - unified media upload library",
  "keywords": [
    "fluxmedia",
    "s3",
    "aws",
    "media",
    "upload",
    "cloud"
  ],
  "homepage": "https://www.fluxmedia.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/codewithveek/fluxmedia.git",
    "directory": "packages/s3"
  },
  "license": "MIT",
  "author": "FluxMedia Contributors",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "!dist/**/*.map"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@fluxmedia/core": "workspace:*"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.980.0",
    "@aws-sdk/lib-storage": "^3.980.0",
    "@types/node": "^22.19.4",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/lib-storage": "^3.0.0",
    "@aws-sdk/s3-request-presigner": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/s3-request-presigner": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
